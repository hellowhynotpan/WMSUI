{
	"easycom": {
			"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
		},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "智能云WMS系统"
			}
		}, {
			"path": "pages/inventoryManagement/inventoryManagement",
			"style": {
				"navigationBarTitleText": "智能云WMS系统",
				"enablePullDownRefresh": true
			}
		}, {
			"path": "pages/baseDataManagement/baseDataManagement",
			"style": {
				"navigationBarTitleText": "智能云WMS系统",
				"enablePullDownRefresh": true
			}
		}, {
			"path": "pages/warehouseManagement/warehouseManagement",
			"style": {
				"navigationBarTitleText": "智能云WMS系统",
				"enablePullDownRefresh": true
			}
		}, {
			"path": "pages/mine/mine",
			"style": {
				"enablePullDownRefresh": true,
				//禁用导航栏
				"app-plus":{
					"titleNView":false
				},
				"navigationBarTextStyle": "white",
				"navigationBarTitleText": "我的",
				"navigationBarBackgroundColor": "#ffd655"
			}
		}
	    ,{
            "path" : "pages/mine/mine",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/auth/login",
            "style" :                                                                                    
            {
               "navigationBarTextStyle": "black",
               "navigationBarTitleText": "登录",
               "navigationBarBackgroundColor": "#ffffff"
            }
        }
    ],
	"globalStyle": {
		//导航栏标题颜色 由于微信小程序的规范只支持黑色和白色  black/white
		"navigationBarTextStyle": "white",
		"navigationBarTitleText": "智能云WMS系统",
		"navigationBarBackgroundColor": "#d81e06",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
		"color": "#bfbfbf",
		"selectedColor": "#515151",
		"backgroundColor": "#ffffff",
		"list": [{
				"pagePath": "pages/index/index",
				"text": "首页",
				"iconPath": "static/images/tabBar/home.png",
				"selectedIconPath":"static/images/tabBar/home-fill.png"
			},
			{
				"pagePath": "pages/baseDataManagement/baseDataManagement",
				"text": "基础数据管理",
				"iconPath": "static/images/tabBar/all.png",
				"selectedIconPath":"static/images/tabBar/all-fill.png"
			},
			{
				"pagePath": "pages/inventoryManagement/inventoryManagement",
				"text": "入库管理",
				"iconPath": "static/images/tabBar/warehouse.png" ,
				"selectedIconPath":"static/images/tabBar/warehouse-fill.png"
			}, {
				"pagePath": "pages/warehouseManagement/warehouseManagement",
				"text": "库存管理",
				"iconPath": "static/images/tabBar/manage-order.png",
				"selectedIconPath":"static/images/tabBar/manage-order-fill.png"
			},
			{
				"pagePath": "pages/mine/mine",
				"text": "我的",
				"iconPath": "static/images/tabBar/mine.png",
				"selectedIconPath":"static/images/tabBar/mine-fill.png"
			}
		]
	}
}
