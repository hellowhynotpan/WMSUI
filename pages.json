	"globalStyle": {
		//导航栏标题颜色 由于微信小程序的规范只支持黑色和白色  black/white
		"navigationBarTextStyle": "white",
		"navigationBarTitleText": "智能云WMS系统",
		"navigationBarBackgroundColor": "#d81e06",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
		"color": "#bfbfbf",
		"selectedColor": "#515151",
		"backgroundColor": "#ffffff",
		"list": [{
